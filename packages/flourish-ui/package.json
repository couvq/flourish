{
  "name": "flourish-ui",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "description": "React component library for the Flourish design system.",
  "author": {
    "name": "Quentin Couvelaire",
    "email": "quentincouvelaire@gmail.com"
  },
  "keywords": [
    "react",
    "component library",
    "design system",
    "flourish",
    "flourish-ui"
  ],
  "license": "MIT",
  "version": "1.0.11",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/couvq/flourish",
    "directory": "packages/flourish-ui"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "build": "npm i && npm run lint && npm run test && rollup -c --bundleConfigAsCjs",
    "test": "jest",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.{js,jsx,ts,tsx}' --write",
    "lint": "eslint src --ext .ts,.tsx",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "flourish-release": "npm run build && npm version patch && npm publish --access public"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/react-vite": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^4.0.0",
    "sass": "^1.66.1",
    "storybook": "^7.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts"
}
